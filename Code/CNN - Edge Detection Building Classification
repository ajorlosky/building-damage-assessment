age_label == \"major-damage\":\n","                major_damage_images.append((cropped_img, 2))\n","            elif damage_label == \"destroyed\":\n","                destroyed_images.append((cropped_img, 3))\n","\n","# Shuffle each list to randomize the order of the images\n","#random.shuffle(no_damage_images)\n","#random.shuffle(minor_damage_images)\n","#random.shuffle(major_damage_images)\n","#random.shuffle(destroyed_images)\n","\n","# Determine the minimum count of images among all damage levels\n","min_count = min(len(no_damage_images), len(minor_damage_images), len(major_damage_images), len(destroyed_images))\n","\n","\n","#palu has no minor damage so remove\n","#min_count = min(len(no_damage_images), len(major_damage_images), len(destroyed_images))\n","#socal has almost no minor or major damage\n","#min_count = min(len(no_damage_images), len(destroyed_images))\n","\n","\n","ratio = 0.8\n","train_count = int(ratio * min_count)\n","\n","# Sample the same number of images from each damage level to balance the dataset\n","#sampled_no_damage = random.sample(no_damage_images, min_count)\n","#sampled_minor_damage = random.sample(minor_damage_images, min_count)\n","#sampled_major_damage = random.sample(major_damage_images, min_count)\n","#sampled_destroyed = random.sample(destroyed_images, min_count)\n","\n","# Split the dataset into train and test according to the provided ratio\n","#train_data = sampled_no_damage[:train_count] + sampled_minor_damage[:train_count] + sampled_major_damage[:train_count] + sampled_destroyed[:train_count]\n","#test_data = sampled_no_damage[train_count:] + sampled_minor_damage[train_count:] + sampled_major_damage[train_count:] + sampled_destroyed[train_count:]\n","\n","#train_data = sampled_no_damage[:train_count] + sampled_major_damage[:train_count] + sampled_destroyed[:train_count]\n","#test_data = sampled_no_damage[train_count:] + sampled_major_damage[train_count:] + sampled_destroyed[train_count:]\n","#train_data = sampled_no_damage[:train_count] +  sampled_destroyed[:train_count]\n","#test_data = sampled_no_damage[train_count:] +  sampled_destroyed[train_count:]\n","#guatamala volcano\n","\n","data = no_damage_images + minor_damage_images + major_damage_images + destroyed_images\n","random.shuffle(data)\n","train_data = data[:int(len(data)*0.8)]\n","test_data = data[int(len(data)*(0.8)):]\n","\n","# Separate the images and labels for train and test sets\n","train_images, train_labels = zip(*train_data)\n","test_images, test_labels = zip(*test_data)\n","\n","# Resize *** this is now done in a different place above  in the looping ***\n","#train_images = make_same_dimensions(train_images, 150)\n","#test_images = make_same_dimensions(test_images, 150)\n"],"metadata":{"id":"V1hxD6mt1JEf"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["## Part 1: Preprocessing of Edge Detection Images from Building Images\n","\n","Transfom the images into images of their edges using edge detection priciples\n","\n","Get the edges of the pre and post disaster and subtract them from each other\n","If a big differece then probably more damage"],"metadata":{"id":"aPHtCUxT0_9O"}},{"cell_type":"code","source":["#reference : https://learnopencv.com/edge-detection-using-opencv/\n","\n","def convert_to_edge(building_img, t = 'Sobel'):\n","  #make the image black and white\n","  #first change to uint8 since thats whats expected here\n","  new_img = np.array(building_img, dtype=np.uint8)\n","\n","  ##TRY some filtering of some kind\n","  kernel_outline = np.array([[-1 ,-1, -1], [-1, 8, -1], [-1, -1, -1]])\n","  kernel_sharpen = np.array([[0, -1, 0], [-1, 5, -1], [0, -1, 0]])\n","  new_img = cv2.filter2D(new_img, -1, kernel_sharpen)\n","  #new_img = cv2.filter2D(new_img, -1, kernel_outline)\n","\n","\n","  #make black and white\n","  new_img = cv2.cvtColor(new_img, cv2.COLOR_BGR2GRAY)\n","\n","  #new_img = cv2.GaussianBlur(new_img,(5,5),cv2.BORDER_DEFAULT)\n","\n","\n","  #do the edge detection depending on the param\n","  if t == 'Sobel':\n","    new_img = cv2.Sobel(src=new_img, ddepth=cv2.CV_64F, dx=2, dy=2, ksize=3)\n","  else:\n","    new_img = cv2.Canny(image=new_img, threshold1=100, threshold2=1000)\n","\n","  return new_img\n","\n","\n","#iterate though the test and the train set doing the edge detection\n","edge_train = []\n","edge_test = []\n","#CHANGE THIS VAR TO CHANGE THE EDGE DETECTION TYPE\n","DETECTION_TYPE = \"Canny\"\n","for i in range(0,len(train_images)):\n","\n","  #subtract the color images first?\n","  #diff = cv2.subtract(train_images[i][0], train_images[i][1])\n","  #diff = np.array(diff, dtype=np.uint8)\n","  #diff = cv2.cvtColor(diff, cv2.COLOR_BGR2GRAY)\n","  img_pre = convert_to_edge(train_images[i][0], t = DETECTION_TYPE)\n","  img_post = convert_to_edge(train_images[i][1], t = DETECTION_TYPE)\n","  diff = cv2.subtract(img_pre,img_post)\n","  #diff = convert_to_edge(diff,t = DETECTION_TYPE)\n","  edge_train.append(diff)\n","\n","\n","for i in range(0,len(test_images)):\n","\n","  #show(test_images[i][0])\n","  #show(test_images[i][1])\n","\n","  #subtract the color images first?\n","  #diff = cv2.subtract(train_images[i][0], train_images[i][1])\n","  #diff = np.array(diff, dtype=np.uint8)\n","  #diff = cv2.cvtColor(diff, cv2.COLOR_BGR2GRAY)\n","\n","  img_pre = convert_to_edge(test_images[i][0], t = DETECTION_TYPE)\n","  img_post = convert_to_edge(test_images[i][1], t = DETECTION_TYPE)\n","\n","\n","  diff = cv2.subtract(img_pre,img_post)\n","  #diff = convert_to_edge(diff,t = DETECTION_TYPE)\n","  edge_test.append(diff)\n","\n","  #print(i, \":\")\n","  #print(\"pre\")\n","  #show(img_pre)\n","  #print(\"post\")\n","  #show(img_post)\n","  #print(\"Label == \", test_labels[i])\n","\n","\n","\n","\n","#show some of the images\n","no_count = 0\n","dmg_count = 0\n","for i in range(0,len(edge_train)):\n","  if train_labels[i] == 0 and no_count < 3:\n","    show(edge_train[i])\n","    print(\"Label == \", train_labels[i])\n","    no_count = no_count + 1\n","  elif train_labels[i] == 3 and dmg_count < 3:\n","    show(edge_train[i])\n","    print(\"Label == \", train_labels[i])\n","    dmg_count = dmg_count + 1\n","\n","  if no_count > 3 and dmg_count > 3:\n","    break"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":505},"id":"8Yu2zK7X1W7z","executionInfo":{"status":"ok","timestamp":1715543250238,"user_tz":240,"elapsed":909,"user":{"displayName":"Pat Marinich","userId":"05922406126454583029"}},"outputId":"9357bcee-2379-4c70-8dc5-1a832b339635"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["<PIL.Image.Image image mode=L size=64x64>"],"image/png":"\n"},"metadata":{}},{"output_type":"stream","name":"stdout","text":["Label ==  0\n"]},{"output_type":"display_data","data":{"text/plain":["<PIL.Image.Image image mode=L size=64x64>"],"image/png":"iVBORw0KGgoAAAANSUhEUgAAAEAAAABACAAAAACPAi4CAAAAeUlEQVR4nO2UQQ6AMAgEW+P/vzwerIlGSC2ol+4k9ATLFpqWIoQQkwD5QloMlRfGiwwRgJMBR7H2lVoS3dSvaPeIT4XbGfGwR9DC0Zr0ajMCuApLXDTiwOAvB2+R3qMwZ7h6iU//H1PgUt3aVmxRvxHdDB+9FyHEpGwGEy3obMkmbAAAAABJRU5ErkJggg==\n"},"metadata":{}},{"output_type":"stream","name":"stdout","text":["Label ==  0\n"]},{"output_type":"display_data","data":{"text/plain":["<PIL.Image.Image image mode=L size=64x64>"],"image/png":"iVBORw0KGgoAAAANSUhEUgAAAEAAAABACAAAAACPAi4CAAAAlUlEQVR4nO2U2wqDQAxEJ8X//+XTh/XSukZ2LEVKc0BYJZmdBBOpKIqiWIjRQIxYSSABQsAm4SiINWc+OQI0D+9fvPsPUjwHjOZPiYTT8UMHbtF7gb4CT5Hx8Iej++sCZhO/4OBmAdJfI5uFl1SFCHM46F7Stp6VQFsscWGydtvoLD8pDUlBe9rB9dApfrYfiqIo/oMnXU876ZaRkmMAAAAASUVORK5CYII=\n"},"metadata":{}},{"output_type":"stream","name":"stdout","text":["Label ==  0\n"]},{"output_type":"display_data","data":{"text/plain":["<PIL.Image.Image image mode=L size=64x64>"],"image/png":"iVBORw0KGgoAAAANSUhEUgAAAEAAAABACAAAAACPAi4CAAAASElEQVR4nO3OMQ4AIAhDUfX+d66rcYFQEwb/W1hoYQwAAB6a8YpSW2GHFXLz1QqT/KtyPz9/qDWlUsuvSCk26ZoN1HgbAPCnDXDLE/Ez1D1jAAAAAElFTkSuQmCC\n"},"metadata":{}},{"output_type":"stream"